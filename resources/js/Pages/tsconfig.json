{
    "compilerOptions": {
        "allowJs": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "jsx": "preserve",
        "strict": true,
        "isolatedModules": true,
        "target": "ESNext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "skipLibCheck": true,
        "paths": {
            "@/*": ["./resources/js/*"],
//            "@advance-table/*": ["./packages/karlos3098/laravel-primevue-table-service/src/Assets/*"],
//            "@advance-table-primevue-dir/*": ["./packages/karlos3098/laravel-primevue-table-service/src/Assets/PrimeVue/4.0.5/*"]
        }
    },
    "include": ["resources/js/**/*.ts", "resources/js/**/*.d.ts", "resources/js/**/*.vue"]
}
